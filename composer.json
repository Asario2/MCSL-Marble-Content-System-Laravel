{
    "name": "starter/eleven",
    "type": "project",
    "description": "Laravel template Starts Eleven by Oliver Reinking.",
    "keywords": [
        "Oliver Reinking",
        "ReinkingConsulting"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "arnoson/kirby-vite": "^5.4",
        "bacon/bacon-qr-code": "^3.0",
        "blaspsoft/socialiteplus": "^1.0",
        "contributte/vite": "^0.2.0",
        "devrabiul/laravel-cookie-consent": "^1.1",
        "endroid/qr-code": "^6.0",
        "envatic/laravel-vue-i18n-generator": "*",
        "ezyang/htmlpurifier": "^4.18",
        "hotwired-laravel/turbo-laravel": "^2.3",
        "illuminate/contracts": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/support": "12.*",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.11",
        "laraeast/laravel-vue-i18n-generator": "*",
        "laravel/fortify": "^1.25",
        "laravel/framework": "12.*",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0.8",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.6",
        "league/commonmark": "^2.7",
        "nextqs/bacon-qr-code": "^2.0",
        "orbitale/imagemagick-php": "^3.3",
        "phpids/phpids": "dev-master",
        "qsfx/bacon-qr-code": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^2.4",
        "reinink/remember-query-strings": "^0.1.2",
        "spatie/laravel-sitemap": "^7.3",
        "stevebauman/purify": "^6.3",
        "tei/photoswipe": "*",
        "tightenco/ziggy": "2.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "^1.23",
        "laravel-lang/lang": "^15.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mammo32_laravel/lang": "^7.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Helper\\": "app/Helper/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "app/Helper/privacybuilder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
		    "security:check": "vendor\\bin\\security-checker security:check composer.lock"

    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": {
        "phpids": {
            "type": "vcs",
            "url": "https://github.com/asario2/phpids.git"
        }
		
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "getkirby/composer-installer": true,
            "typo3/class-alias-loader": false,
            "typo3/cms-composer-installers": false
        }
    }
}
